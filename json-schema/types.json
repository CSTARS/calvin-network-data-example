{
    "id":"http://cws.ucdavis.edu/types.json",
    "title":"Types",
    "description":"Types are single valued objects with particular requirements.",
    "anyOf":[
	{"$ref" : "#/prmname"},
	{"$ref" : "#/date"},
	{"$ref" : "#/month"},
	{"$ref" : "#/surface_area"},
	{"$ref" : "#/elevation"},
	{"$ref" : "#/capacity"}
    ],
    "prmname" : {
	"description": "Local identifier for the object, (historically named prmname).",
	"type": "string",
	"pattern": "^([a-z]|[A-Z]|[0-9]|[_])+(-([a-z]|[A-Z]|[0-9]|[_])+)$"
    },
    "capacity": {
	"description":"Capacities in the network are non-negative, and are all referenced in KAF.",
	"oneOf":[
	    {"type":"null"},
	    {
		"type":"number",
		"minimum":0
	    }
	]
    },
    "date": {
	"description":"IETF RFC 3339 date specification,http://tools.ietf.org/html/rfc3339",
	"type" : "string",
	"format" : "date-time"
    },
    "month": {
	"description":"Three letter month designation.",
	"enum" : ["JAN","FEB","MAR","APR","MAY","JUN","JUL","AUG","SEP","OCT","NOV","DEC"]
    },
    "surface_area": {
	"description":"Surface Area of Storage in thousand acres [kA]",
	"oneOf":[
	    {"type":"null"},
	    {
		"type":"number",
		"minimum":0
	    }
	]
    },
    "elevation": {
	"description":"Elevation of Surface [ft]",
	"oneOf":[
	    {"type":"null"},
	    {
		"type":"number",
		"minimum":0
	    }
	]
    }
}
